<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <bean id="appconfig" class="com.kingsoft.sjk.config.AppConfig">
        <property name="appPageBaseUrl" value="${app-page-base-url}" />
        <property name="appImgBaseurl" value="${app-img-base-url}" />
        <property name="appImgStorePath" value="${app-img-store-path}" />
        <property name="excelBasePath" value="${excel-base-path}" />
        <property name="resultInfoPath" value="${result-info-path}" />
    </bean>
</beans>