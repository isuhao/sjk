<?xml version="1.0" encoding="utf-8"?>
<assembly>
	<id>zip-classes</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>target/classes/com</directory>
			<outputDirectory>/com</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>target/classes/sql-file</directory>
			<outputDirectory>/sql-file</outputDirectory>
		</fileSet>
	</fileSets>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/lib</outputDirectory>
			<includes>
				<include>*:jar:*</include>
			</includes>
		</dependencySet>
	</dependencySets>
</assembly>