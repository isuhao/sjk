<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	<bean id="appConfig" class="com.kingsoft.sjk.config.AppConfig">
        <property name="appPageBaseUrl" value="${app-page-base-url}" /> 
        <property name="generateDate" value="${generate-date}" />
        <property name="appGenerateTemplateBaseDir" value="${app-generate-template-base-dir}" />
        <property name="appDetailGenerateBasePath" value="${app-detail-generate-base-path}" />
        <property name="pagesize" value="${pagesize}" />
        <property name="debug" value="${debug}" />
	</bean>
</beans>